<div class="container">
  <div class="row">
    <div class="col-md-12">
      <%= form_for @note do |f| %>
      <p>Title: <%= f.text_field :title %></p>
      <p>Body: <%= f.text_area :body, cols: 20, rows: 10%></p>
      <%= f.submit %>
      <% end %>

      <%= link_to 'Home', root_path %></br>
      <%= link_to('Log out', destroy_user_session_path, method: :delete) %></br>
    </div>
  </div>
</div>